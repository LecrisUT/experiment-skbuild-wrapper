from __future__ import annotations

import subprocess
import sys
from pathlib import Path

_runtime_dlls = "$<TARGET_RUNTIME_DLLS:@target@>"

def run() -> None:
    exe_path = Path(__file__).parent
    exe_path = exe_path / "@CMAKE_INSTALL_BINDIR@/$<TARGET_FILE_NAME:@target@>"
    sys.exit(
        subprocess.call(
            [str(exe_path)],
        )
    )
